  <SQLQuery><![CDATA[select    
    r.reference_id,    
    r.contract_source,    
    r.reference_number,    
    (select xlvt.meaning from xxcust_lookup_values_t xlvt where xlvt.lookup_type = 'XXADS_CUSTOMER_TYPES' and xlvt.lookup_code = r.customer_type and xlvt.application_id = 13) trans_customer_type , 
    r.customer_type,    
    (select xpj.project_number||' - '||xpj.project_name from xxpj_projects_t xpj where xpj.project_id = r.PROJECT_ID) trans_project,   
    r.project_id,    
    (select fv.description from fnd_flex_values_vl fv where fv.flex_value_id = r.DIV_FLEX_VALUE_ID) trans_division,   
    r.div_flex_value_id,    
    r.DIV_FLEX_VAL_SET_ID,   
    (select fv.flex_value||' - '|| fv.description from fnd_flex_values_vl fv where fv.flex_value_id = r.DEPT_FLEX_VALUE_ID) trans_department,   
    r.department_name,   
    r.department_code,   
    r.dept_flex_value_id,    
    r.DEPT_FLEX_VALUE_SET_ID,   
    (select xcdt.customer_name from xxfi_customer_details_t xcdt where xcdt.cust_account_id = r.CUSTOMER_ID) trans_customer,   
    r.customer_id,    
    (SELECT xcsdt.bill_to_site||' - '||xcsdt.address from xxfi_customer_site_details_t xcsdt where xcsdt.site_use_id = r.SITE_USE_ID) trans_customer_site,   
    r.site_use_id,    
    r.CUST_SITE_ID,   
    r.master_id,  
    r.parent_id  
from     
    (select     
        quotation_id reference_id,    
        'Q' contract_source,    
        quote_number reference_number,    
        customer_type,    
        project_id,    
        div_flex_value_id,    
        dept_flex_value_id,    
        customer_id,    
        site_use_id,            
        master_quote_id master_id,   
        parent_quote_id parent_id,  
        bu_id,    
        req_bu_id,    
        src_div_flex_val_id,    
        quote_type contract_type ,   
        cust_site_id,   
        department_name,   
        department_code,   
        div_flex_val_set_id,   
        dept_flex_value_set_id   
    from    
        xxads_rental_quote_t    
    where     
        quote_status = 'A'    
    and active_flag = 'Y'    
    union    
    select     
        request_id reference_id,    
        'R' contract_source,    
        request_number reference_number,    
        customer_type,    
        project_id,    
        div_flex_value_id,    
        dept_flex_value_id,    
        customer_id,    
        site_use_id,    
        null,    
        null,  
        source_bu_id,    
        requestor_bu_id,    
        src_div_flex_value_id,    
        request_type contract_type,   
        cust_site_id,   
        department_name,   
        department_code,   
        div_flex_value_set_id,   
        dept_flex_value_set_id   
    from    
        xxads_asset_request_t    
    where     
        request_status = 'A') r    
where     
    r.contract_type = :p_contract_type    
and r.bu_id = :p_src_bu_id    
and nvl(r.src_div_flex_val_id,-1) = nvl(:p_src_div_flex_val_id,nvl(r.src_div_flex_val_id,-1))    
and r.req_bu_id = :p_req_bu_id  
and r.contract_source = :p_contract_source]]></SQLQuery>
