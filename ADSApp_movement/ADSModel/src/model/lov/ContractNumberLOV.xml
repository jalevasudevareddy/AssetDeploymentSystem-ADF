  <SQLQuery><![CDATA[select xrct.contract_number contract_number,                    
          xrct.contract_id,                    
          xrct.master_contract_id,                   
          xrct.CUSTOMER_TYPE,                   
          (select xlvt.meaning from xxcust_lookup_values_t xlvt where xlvt.lookup_type = 'XXADS_CUSTOMER_TYPES' and xlvt.lookup_code = xrct.CUSTOMER_TYPE and xlvt.application_id = 13) customer_type_meaning,                   
          xrct.PROJECT_ID,                   
          (select xpj.project_number||' - '||xpj.project_name from xxpj_projects_t xpj where xpj.project_id = xrct.PROJECT_ID) project_name,                   
          (select fv.description from fnd_flex_values_vl fv where fv.flex_value_id = xrct.DIV_FLEX_VALUE_ID) DIVISION,                   
          xrct.DIV_FLEX_VALUE_ID,                   
          xrct.DIV_FLEX_VAL_SET_ID,                   
          xrct.DEPARTMENT_NAME,                   
          xrct.DEPARTMENT_CODE,                  
          xrct.DEPT_FLEX_VALUE_ID,                   
          xrct.DEPT_FLEX_VALUE_SET_ID,            
          (select fv.flex_value||' - '|| fv.description from fnd_flex_values_vl fv where fv.flex_value_id = xrct.DEPT_FLEX_VALUE_ID) dept_name,           
          xrct.customer_id,                   
          (select xcdt.customer_name from xxfi_customer_details_t xcdt where xcdt.cust_account_id = xrct.CUSTOMER_ID) customer_name,                   
          xrct.cust_site_id,                   
          xrct.site_use_id,                   
          (SELECT xcsdt.bill_to_site||' - '||xcsdt.address from xxfi_customer_site_details_t xcsdt where xcsdt.site_use_id = xrct.SITE_USE_ID) customer_site                   
from xxads_rental_contract_t xrct                     
where (:p_page_type <> 'S'                     
        or (                     
                :p_page_type = 'S'                     
                and exists (select 'Y' from xxads_asset_allocation_t xaat where xaat.contract_id= xrct.contract_id)                     
            ))                     
        and xrct.src_bu_id = :p_src_bu_id                         
        and nvl(xrct.src_div_flex_val_id,-1) = nvl(:p_src_div_flex_val_id,nvl(xrct.src_div_flex_val_id,-1))                         
        and xrct.req_bu_id = :p_req_bu_id                     
        and ((nvl(active_flag,'N') = 'Y' and nvl(xrct.contract_status,'D') = 'A')                  
         or (nvl(:p_trxn_page,'O')='A' and nvl(xrct.contract_status,'D') = 'CH' and xrct.contract_id = xrct.master_contract_id))]]></SQLQuery>
