and nvl(XxadsRentalContractEO.CUSTOMER_ID,-1) = nvl(:p_customer_id, nvl(XxadsRentalContractEO.CUSTOMER_ID,-1))        
and nvl(XxadsRentalContractEO.SITE_USE_ID,-1) = nvl(:p_site_use_id, nvl(XxadsRentalContractEO.SITE_USE_ID,-1))   
and XxadsRentalContractEO.VALID_FROM = NVL(to_date(:p_effective_from),XxadsRentalContractEO.VALID_FROM)   
and NVL(XxadsRentalContractEO.VALID_TO, TO_DATE('31-12-4712','DD-MM-YYYY')) = nvl(to_date(:p_effective_to), NVL(XxadsRentalContractEO.VALID_TO, TO_DATE('31-12-4712','DD-MM-YYYY'))) 
and exists (select xxads_process_pkg.validate_ads_access(XxadsRentalContractEO.CUSTOMER_TYPE, 
                                                                                        (case  
