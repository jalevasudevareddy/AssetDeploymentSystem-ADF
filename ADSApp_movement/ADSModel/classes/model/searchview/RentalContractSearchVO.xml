                                                                                            when 
                                                                                                XxadsRentalContractEO.CUSTOMER_TYPE = 'C' 
                                                                                            then  
                                                                                                XxadsRentalContractEO.CUSTOMER_ID 
                                                                                            when  
                                                                                                XxadsRentalContractEO.CUSTOMER_TYPE = 'D' 
                                                                                            then 
                                                                                                XxadsRentalContractEO.DEPT_FLEX_VALUE_ID 
                                                                                            when 
                                                                                                XxadsRentalContractEO.CUSTOMER_TYPE = 'DV' 
                                                                                            then 
                                                                                                XxadsRentalContractEO.DIV_FLEX_VALUE_ID 
                                                                                            else 
                                                                                                null 
                                                                                        end),                                                                                      
                                                                                        XxadsRentalContractEO.SRC_BU_ID,  
                                                                                        XxadsRentalContractEO.SRC_DIV_FLEX_VAL_ID, 
                                                                                        XxadsRentalContractEO.REQ_BU_ID,  
                                                                                        :p_user_id)  
                        from dual)">
