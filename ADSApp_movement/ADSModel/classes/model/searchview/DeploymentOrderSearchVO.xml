       (select xlvt.meaning from xxcust_lookup_values_t xlvt where xlvt.lookup_type = 'XXADS_DEPLOYMENT_ORDER_STATUS'  and xlvt.lookup_code = XxadsDoHeaderEO.do_status and xlvt.application_id = 13) AS TRANSADOSTATUS, 
       (select fv.flex_value||' - '|| fv.description from fnd_flex_values_vl fv where fv.flex_value_id = XxadsDoHeaderEO.DEPT_FLEX_VALUE_ID) AS TRANSDEPTNAME, 
       (select nvl((to_number(substr(XxadsDoHeaderEO.DO_NUMBER,instr(XxadsDoHeaderEO.DO_NUMBER,'-',-1)+1))),0) from dual) AS TRANSADONUMBER"
