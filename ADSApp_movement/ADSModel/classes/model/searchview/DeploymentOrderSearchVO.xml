and XxadsDoHeaderEO.CONTRACT_ID = nvl(:p_contract_id, XxadsDoHeaderEO.CONTRACT_ID)       
and XxadsDoHeaderEO.CUSTOMER_TYPE = nvl(:p_customer_type, XxadsDoHeaderEO.CUSTOMER_TYPE)              
and nvl(XxadsDoHeaderEO.PROJECT_ID,-1) = nvl(:p_project_id, nvl(XxadsDoHeaderEO.PROJECT_ID,-1))              
and nvl(XxadsDoHeaderEO.DIV_FLEX_VALUE_ID,-1) = nvl(:p_div_flex_val_id, nvl(XxadsDoHeaderEO.DIV_FLEX_VALUE_ID,-1))             
