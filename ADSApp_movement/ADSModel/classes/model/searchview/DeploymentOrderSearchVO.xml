and nvl(XxadsDoHeaderEO.SRC_DIV_FLEX_VALUE_ID,-2) = NVL(:p_src_div_flex_val_id,nvl(XxadsDoHeaderEO.SRC_DIV_FLEX_VALUE_ID,-2))              
