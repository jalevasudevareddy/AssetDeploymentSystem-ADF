and nvl(XxadsDoHeaderEO.CUSTOMER_ID,-1) = nvl(:p_customer_id, nvl(XxadsDoHeaderEO.CUSTOMER_ID,-1))             
and XxadsDoHeaderEO.DO_NUMBER = nvl(:p_do_number, XxadsDoHeaderEO.DO_NUMBER)       
and XxadsDoHeaderEO.DOCUMENT_DATE = NVL(to_date(:p_document_date),XxadsDoHeaderEO.DOCUMENT_DATE)   
and exists (select xxads_process_pkg.validate_ads_access(XxadsDoHeaderEO.CUSTOMER_TYPE,   
                                                                                        (case    
                                                                                            when    
                                                                                                XxadsDoHeaderEO.CUSTOMER_TYPE = 'P'    
