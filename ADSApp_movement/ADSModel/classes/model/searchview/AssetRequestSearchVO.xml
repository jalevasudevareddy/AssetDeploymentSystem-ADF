       XxadsAssetRequestEO.SUBMITTED_BY, 
       XxadsAssetRequestEO.SUBMITTED_DATE, 
       (select inv.BU_NAME from xxinv_business_units_t inv where inv.BU_ID = XxadsAssetRequestEO.REQUESTOR_BU_ID) AS VIEW_ATTR, 
       (select inv.BU_NAME from xxinv_business_units_t inv where inv.BU_ID = XxadsAssetRequestEO.SOURCE_BU_ID) AS VIEW_ATTR, 
       (SELECT xlvt.MEANING from xxcust_lookup_values_t xlvt where xlvt.LOOKUP_TYPE = 'XXADS_REQUEST_STATUS' and xlvt.LOOKUP_CODE = XxadsAssetRequestEO.REQUEST_STATUS) AS VIEW_ATTR, 
       (SELECT xlvt.MEANING from xxcust_lookup_values_t xlvt where xlvt.LOOKUP_TYPE = 'XXADS_CUSTOMER_TYPES' and xlvt.LOOKUP_CODE = XxadsAssetRequestEO.CUSTOMER_TYPE) AS VIEW_ATTR, 
       (select a.customer_name from xxfi_customer_details_t a where a.cust_account_id = XxadsAssetRequestEO.CUSTOMER_ID) AS VIEW_ATTR, 
       (SELECT  BILL_TO_SITE|| ' - ' || ADDRESS site_address FROM  xxFI_customer_site_details_t WHERE cust_acct_site_id =XxadsAssetRequestEO.CUST_SITE_ID and bu_id = XxadsAssetRequestEO.source_bu_id and site_use_id = XxadsAssetRequestEO.SITE_USE_ID) AS VIEW_ATTR, 
       (select PJT.PROJECT_NAME code_desc FROM XXPJ_PROJECTS_T PJT WHERE PJT.BU_ID   = XxadsAssetRequestEO.source_bu_id AND PJT.project_id  = XxadsAssetRequestEO.project_id AND upper(NVL(PJT.TEMPLATE_FLAG,'N')) = 'N' AND XXPJ_STATUS_CONTROL_FUN(PJT.PROJECT_ID)='Y') AS VIEW_ATTR, 
       (select nvl((to_number(substr(XxadsAssetRequestEO.REQUEST_NUMBER,instr(XxadsAssetRequestEO.REQUEST_NUMBER,'-',-1)+1))),0) from dual) AS VIEW_ATTR"
  FromList="XXADS_ASSET_REQUEST_T XxadsAssetRequestEO"
  Where="XxadsAssetRequestEO.REQUEST_TYPE = :p_request_type       
        and XxadsAssetRequestEO.REQUESTOR_BU_ID = :p_requestor_bu_id       
        and XxadsAssetRequestEO.SOURCE_BU_ID = :p_src_bu_id       
        and nvl(XxadsAssetRequestEO.SRC_DIV_FLEX_VAL_SET_ID, -101) = nvl(:p_src_div_flex_value_set_id,nvl(XxadsAssetRequestEO.SRC_DIV_FLEX_VAL_SET_ID, -101))        
        and nvl(XxadsAssetRequestEO.SRC_DIV_FLEX_VALUE_ID,-100) = nvl(:p_src_div_flex_value_id, nvl(XxadsAssetRequestEO.SRC_DIV_FLEX_VALUE_ID,-100))       
        and XxadsAssetRequestEO.REQUEST_NUMBER = nvl(:p_request_num, XxadsAssetRequestEO.REQUEST_NUMBER)       
        and XxadsAssetRequestEO.REQUEST_STATUS = nvl(:p_request_status, XxadsAssetRequestEO.REQUEST_STATUS)       
        and XxadsAssetRequestEO.REQUESTED_DATE between nvl(:p_requested_from,XxadsAssetRequestEO.REQUESTED_DATE) and nvl(:p_requested_to,XxadsAssetRequestEO.REQUESTED_DATE)       
        and XxadsAssetRequestEO.CUSTOMER_TYPE = nvl(:p_customer_type,XxadsAssetRequestEO.CUSTOMER_TYPE)       
        and nvl(XxadsAssetRequestEO.PROJECT_ID,-99) = nvl(:p_project_id,  nvl(XxadsAssetRequestEO.PROJECT_ID,-99) )       
        and nvl(XxadsAssetRequestEO.DIV_FLEX_VALUE_SET_ID, -98) = nvl(:p_div_flex_value_set_id,nvl(XxadsAssetRequestEO.DIV_FLEX_VALUE_SET_ID, -98))        
        and nvl(XxadsAssetRequestEO.DIV_FLEX_VALUE_ID,-97) = nvl(:p_div_flex_value_id, nvl(XxadsAssetRequestEO.DIV_FLEX_VALUE_ID,-97))       
        and nvl(XxadsAssetRequestEO.DEPT_FLEX_VALUE_SET_ID, -96) = nvl(:p_dept_flex_value_set_id,nvl(XxadsAssetRequestEO.DEPT_FLEX_VALUE_SET_ID, -96))       
        and nvl(XxadsAssetRequestEO.DEPT_FLEX_VALUE_ID,-95) = nvl(:p_dept_flex_value_id, nvl(XxadsAssetRequestEO.DEPT_FLEX_VALUE_ID,-95))      
        and nvl(XxadsAssetRequestEO.CUSTOMER_ID,-94) = nvl(:p_cust_account_id,  nvl(XxadsAssetRequestEO.CUSTOMER_ID,-94))   
        and exists (select xxads_process_pkg.validate_ads_access(XxadsAssetRequestEO.CUSTOMER_TYPE,   
                                                                                                (case    
                                                                                                    when    
                                                                                                        XxadsAssetRequestEO.CUSTOMER_TYPE = 'P'    
                                                                                                    then    
                                                                                                        XxadsAssetRequestEO.PROJECT_ID   
                                                                                                    when   
                                                                                                        XxadsAssetRequestEO.CUSTOMER_TYPE = 'C'   
                                                                                                    then    
                                                                                                        XxadsAssetRequestEO.CUSTOMER_ID   
                                                                                                    when    
                                                                                                        XxadsAssetRequestEO.CUSTOMER_TYPE = 'D'   
                                                                                                    then   
                                                                                                        XxadsAssetRequestEO.DEPT_FLEX_VALUE_ID   
                                                                                                    when   
                                                                                                        XxadsAssetRequestEO.CUSTOMER_TYPE = 'DV'   
                                                                                                    then   
                                                                                                        XxadsAssetRequestEO.DIV_FLEX_VALUE_ID   
                                                                                                    else   
                                                                                                        null   
                                                                                                end),                                                                                        
                                                                                                XxadsAssetRequestEO.SOURCE_BU_ID,    
                                                                                                XxadsAssetRequestEO.SRC_DIV_FLEX_VALUE_ID,   
                                                                                                XxadsAssetRequestEO.REQUESTOR_BU_ID,    
                                                                                                :p_user_id)    
                        from dual)">
