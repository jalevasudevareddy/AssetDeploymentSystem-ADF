and xlvt.lookup_code = XxadsAssetAllocationEO.customer_type and xlvt.application_id = 13) AS TRANSCUSTOMERTYPE, 
       (select xpj.project_number||' - '||xpj.project_name from xxpj_projects_t xpj where xpj.project_id = XxadsAssetAllocationEO.PROJECT_ID) AS TRANSPROJECT, 
       (select xcdt.customer_name from xxfi_customer_details_t xcdt where xcdt.cust_account_id = XxadsAssetAllocationEO.CUSTOMER_ID) AS TRANSCUSTOMERNAME, 
       (SELECT xcsdt.bill_to_site||' - '||xcsdt.address from xxfi_customer_site_details_t xcsdt where xcsdt.site_use_id = XxadsAssetAllocationEO.SITE_USE_ID) AS TRANSCUSTOMERSITE, 
       (select fv.flex_value||' - '|| fv.description from fnd_flex_values_vl fv where fv.flex_value_id = XxadsAssetAllocationEO.DEPT_FLEX_VALUE_ID) AS TRANSDEPTNAME, 
       (select xlvt.meaning from xxcust_lookup_values_t xlvt where xlvt.lookup_type = 'XXADS_ASSET_ALLOCATION_STATUS' and xlvt.lookup_code = XxadsAssetAllocationEO.ALLOCATION_STATUS and xlvt.application_id = 13) AS TRANSALLOCATIONSTATUS, 
       (select count(*) from por_requisition_headers_all 
where requisition_header_id in (select  pr_header_id from xxads_asset_allocation_dtls_t where allocation_id=XxadsAssetAllocationEO.allocation_id)
and document_status &lt;> 'REJECTED') AS TRANSPRINTERFACEDDTLCOUNT, 
       (select nvl((to_number(substr(allocation_number,instr(allocation_number,'-',-1)+1))),0) from dual) AS VIEW_ATTR"
  FromList="XXADS_ASSET_ALLOCATION_T XxadsAssetAllocationEO"
  Where="XxadsAssetAllocationEO.SRC_BU_ID = :p_src_bu_id            
and nvl(XxadsAssetAllocationEO.SRC_DIV_FLEX_VALUE_ID,-2) = NVL(:p_src_div_flex_val_id,nvl(XxadsAssetAllocationEO.SRC_DIV_FLEX_VALUE_ID,-2))            
and XxadsAssetAllocationEO.REQ_BU_ID = nvl(:p_req_bu_id, XxadsAssetAllocationEO.REQ_BU_ID)            
and XxadsAssetAllocationEO.CONTRACT_ID = nvl(:p_contract_id, XxadsAssetAllocationEO.CONTRACT_ID)     
and XxadsAssetAllocationEO.CUSTOMER_TYPE = nvl(:p_customer_type, XxadsAssetAllocationEO.CUSTOMER_TYPE)            
and nvl(XxadsAssetAllocationEO.PROJECT_ID,-1) = nvl(:p_project_id, nvl(XxadsAssetAllocationEO.PROJECT_ID,-1))            
and nvl(XxadsAssetAllocationEO.DIV_FLEX_VALUE_ID,-1) = nvl(:p_div_flex_val_id, nvl(XxadsAssetAllocationEO.DIV_FLEX_VALUE_ID,-1))           
and nvl(XxadsAssetAllocationEO.DEPT_FLEX_VALUE_ID,-1) = nvl(:p_dept_flex_val_id, nvl(XxadsAssetAllocationEO.DEPT_FLEX_VALUE_ID,-1))           
and nvl(XxadsAssetAllocationEO.CUSTOMER_ID,-1) = nvl(:p_customer_id, nvl(XxadsAssetAllocationEO.CUSTOMER_ID,-1))           
and XxadsAssetAllocationEO.ALLOCATION_NUMBER = nvl(:p_allocation_number, XxadsAssetAllocationEO.ALLOCATION_NUMBER)     
and XxadsAssetAllocationEO.ALLOCATION_DATE = NVL(to_date(:p_allocation_date),XxadsAssetAllocationEO.ALLOCATION_DATE)  
and exists (select xxads_process_pkg.validate_ads_access(XxadsAssetAllocationEO.CUSTOMER_TYPE,  
                                                                                                (case   
                                                                                                    when   
                                                                                                        XxadsAssetAllocationEO.CUSTOMER_TYPE = 'P'   
                                                                                                    then   
                                                                                                        XxadsAssetAllocationEO.PROJECT_ID  
                                                                                                    when  
                                                                                                        XxadsAssetAllocationEO.CUSTOMER_TYPE = 'C'  
                                                                                                    then   
                                                                                                        XxadsAssetAllocationEO.CUSTOMER_ID  
                                                                                                    when   
                                                                                                        XxadsAssetAllocationEO.CUSTOMER_TYPE = 'D'  
                                                                                                    then  
                                                                                                        XxadsAssetAllocationEO.DEPT_FLEX_VALUE_ID  
                                                                                                    when  
                                                                                                        XxadsAssetAllocationEO.CUSTOMER_TYPE = 'DV'  
                                                                                                    then  
                                                                                                        XxadsAssetAllocationEO.DIV_FLEX_VALUE_ID  
                                                                                                    else  
                                                                                                        null  
                                                                                                end),                                                                                       
                                                                                                XxadsAssetAllocationEO.SRC_BU_ID,   
                                                                                                XxadsAssetAllocationEO.SRC_DIV_FLEX_VALUE_ID,  
                                                                                                XxadsAssetAllocationEO.REQ_BU_ID,   
                                                                                                :p_user_id)   
                        from dual)">
