       XxadsRentalContractLinesEO.OFF_HIRE_DATE, 
       XxadsRentalContractLinesEO.ON_HIRE_DATE, 
       XxadsRentalContractLinesEO.OPERATOR_OPTION, 
       XxadsRentalContractLinesEO.OT1_RATE, 
       XxadsRentalContractLinesEO.OT2_RATE, 
       XxadsRentalContractLinesEO.PARENT_CONTRACT_ID, 
       XxadsRentalContractLinesEO.PARENT_CONTRACT_LINE_ID, 
       XxadsRentalContractLinesEO.QUANTITY, 
       XxadsRentalContractLinesEO.REFERENCE_ID, 
       XxadsRentalContractLinesEO.REFERENCE_LINE_ID, 
       XxadsRentalContractLinesEO.REFERENCE_LINE_NUM, 
       XxadsRentalContractLinesEO.REMARKS, 
       XxadsRentalContractLinesEO.RENTAL_TYPE, 
       XxadsRentalContractLinesEO.SHIFT, 
       XxadsRentalContractLinesEO.STANDARD_RATE, 
       (select meaning from xxcust_lookup_values_t where lookup_type = 'XXADS_RENTAL_TYPE' and application_id = 13 and lookup_code = XxadsRentalContractLinesEO.RENTAL_TYPE) AS TRANSRENTALTYPE, 
       (case when XxadsRentalContractLinesEO.rental_type &lt;> 'O' then (select asset_category from xxads_asset_category_t where category_id=XxadsRentalContractLinesEO.CATEGORY_ID)     when XxadsRentalContractLinesEO.rental_type = 'O' then (select emp_category_code from xxads_emp_category_t where emp_category_id=XxadsRentalContractLinesEO.CATEGORY_ID)     end) AS TRANSCATEGORY, 
       (case when XxadsRentalContractLinesEO.rental_type &lt;> 'O' then (select description from xxads_asset_category_t where category_id=XxadsRentalContractLinesEO.CATEGORY_ID)     when XxadsRentalContractLinesEO.rental_type = 'O' then (select description from xxads_emp_category_t where emp_category_id=XxadsRentalContractLinesEO.CATEGORY_ID)     end) AS TRANSCATEGORYDESCRIPTION, 
       (select hiring_basis from XXADS_HIRING_BASIS_RULES_T where rule_id=XxadsRentalContractLinesEO.HIRING_BASIS_RULE_ID) AS TRANSHIRINGBASIS, 
       (SELECT XLVT.MEANING FROM XXCUST_LOOKUP_VALUES_T xlvt, XXADS_HIRING_BASIS_RULES_T a where xlvt.lookup_type = 'XXADS_FIXED_VARIABLE' and xlvt.lookup_code = a.FIXED_VARIABLE and a.RULE_ID = XxadsRentalContractLinesEO.HIRING_BASIS_RULE_ID AND xlvt.application_id = 13) AS TRANSFIXEDVARIABLE, 
       (case when XxadsRentalContractLinesEO.OPERATOR_OPTION='W' then 'With' else 'Without' end) AS TRANSOPERATOROPTION, 
       (case when XxadsRentalContractLinesEO.FUEL_OPTION='W' then 'With' else 'Without' end) AS TRANSFUELOPTION, 
       (case when XxadsRentalContractLinesEO.rental_type &lt;> 'O' then (select lookup.meaning from xxads_asset_category_t cat, xxcust_lookup_values_t lookup where cat.category_id=XxadsRentalContractLinesEO.CATEGORY_ID and lookup.lookup_code = nvl(mobilization_flag,'N') and lookup.application_id = 13 and lookup.lookup_type = 'XXADS_YES_NO')     when XxadsRentalContractLinesEO.rental_type = 'O' then null end) AS TRANSMOBILIZATIONREQUIRED, 
       (select meaning from xxcust_lookup_values_t where lookup_type = 'XXADS_SHIFT_TYPE' and application_id = 13 and lookup_code = XxadsRentalContractLinesEO.SHIFT) AS TRANSSHIFT, 
       (select xlvt.meaning from xxcust_lookup_values_t xlvt where xlvt.lookup_type IN ('XXADS_QUOTE_CHARGEABLE_UNITS','XXADS_HIRING_PERIOD_ID') and xlvt.lookup_code = XxadsRentalContractLinesEO.Min_Charge_Unit and xlvt.application_id = 13) AS TRANSMINCHARGEUNIT, 
       (case XxadsRentalContractEO.CONTRACT_SOURCE when 'Q' then (select quote_number from xxads_rental_quote_t where quotation_id= XxadsRentalContractLinesEO.REFERENCE_ID) WHEN 'R' then (select request_number from xxads_asset_request_t where request_id = XxadsRentalContractLinesEO.REFERENCE_ID) else null end) AS TRANSREFERENCENUMBER, 
       XxadsRentalContractEO.SRC_BU_ID, 
       XxadsRentalContractEO.CONTRACT_ID AS CONTRACT_ID1, 
       XxadsRentalContractEO.CONTRACT_SOURCE, 
       XxadsRentalContractEO.SRC_DIV_FLEX_VAL_ID, 
       XxadsRentalContractLinesEO.MASTER_REF_LINE_ID, 
       XxadsRentalContractLinesEO.TASK_ID, 
       XxadsRentalContractLinesEO.EXPENDITURE_TYPE_ID, 
       (select task_name from XXPJ_PROJECT_TASKS_T where task_id = XxadsRentalContractLinesEO.TASK_ID  and project_id = XxadsRentalContractEO.project_id) AS TRANSTASK, 
       (select expenditure_type_name from XXPJ_EXPENDITURE_TYPES_T where expenditure_type_id =XxadsRentalContractLinesEO.EXPENDITURE_TYPE_ID) AS TRANSEXPENDITUREDESC, 
       (select set_id from XXINV_BUSINESS_UNITS_T where bu_id = XxadsRentalContractEO.REQ_BU_ID) AS TRANSSETID, 
       XxadsRentalContractEO.PROJECT_ID, 
       XxadsRentalContractLinesEO.UNIT_RATE, 
       (select no_of_days from XXADS_HIRING_BASIS_RULES_T where rule_id=XxadsRentalContractLinesEO.HIRING_BASIS_RULE_ID) AS TRANSNOOFDAYS, 
       (select no_of_hours from XXADS_HIRING_BASIS_RULES_T where rule_id=XxadsRentalContractLinesEO.HIRING_BASIS_RULE_ID) AS TRANSSTANDARDHOURS, 
       XxadsRentalContractLinesEO.TAX_RATE_ID, 
       XxadsRentalContractLinesEO.TAX_RATE_CODE, 
       XxadsRentalContractLinesEO.TAX_RATE_PERCENT, 
       XxadsRentalContractLinesEO.VAT_TAX_AMOUNT, 
       XxadsRentalContractLinesEO.CATEGORY_DESCRIPTION"
  FromList="XXADS_RENTAL_CONTRACT_LINES_T XxadsRentalContractLinesEO, XXADS_RENTAL_CONTRACT_T XxadsRentalContractEO"
  Where="XxadsRentalContractEO.CONTRACT_ID =XxadsRentalContractLinesEO.CONTRACT_ID"
