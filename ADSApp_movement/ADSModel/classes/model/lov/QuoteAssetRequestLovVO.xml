  <SQLQuery><![CDATA[select hdr.request_number, hdr.request_id, hdr.source_bu_id, hdr.requestor_bu_id, hdr.src_div_flex_value_id    
from xxads_asset_request_t hdr where hdr.request_status = 'A'    
and exists (select 'Y' from xxads_asset_request_lines_t lns where lns.request_id = hdr.request_id and lns.line_status = 'P')    
and hdr.source_bu_id = :p_src_bu_id    
and hdr.requestor_bu_id = :p_req_bu_id    
and nvl(hdr.src_div_flex_value_id,-2) = nvl(:p_src_div_flex_val_id,-2) 
and hdr.customer_type = :p_customer_type   
and nvl(hdr.project_id, -1)=nvl(:p_project_id, nvl(hdr.project_id, -1))   
and nvl(hdr.div_flex_value_id, -1)=nvl(:p_div_flex_value_id, nvl(hdr.div_flex_value_id, -1))   
and nvl(hdr.dept_flex_value_id, -1)=nvl(:p_dept_flex_value_id, nvl(hdr.dept_flex_value_id, -1))   
and nvl(hdr.customer_id, -1)=nvl(:p_customer_id, nvl(hdr.customer_id, -1))   
and nvl(hdr.site_use_id, -1)=nvl(:p_site_use_id, nvl(hdr.site_use_id, -1))]]></SQLQuery>
