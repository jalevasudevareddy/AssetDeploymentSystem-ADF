       XxadsRentalQuoteEO.REVISED_ON, 
       XxadsRentalQuoteEO.REVISION_NUMBER, 
       XxadsRentalQuoteEO.REVISION_REASON, 
       XxadsRentalQuoteEO.SITE_USE_ID, 
       XxadsRentalQuoteEO.SRC_DIV_FLEX_VAL_ID, 
       XxadsRentalQuoteEO.SRC_DIV_FLEX_VAL_SET_ID, 
       XxadsRentalQuoteEO.SRC_DIVISION, 
       XxadsRentalQuoteEO.SUBMITTED_BY, 
       XxadsRentalQuoteEO.SUBMITTED_DATE, 
       XxadsRentalQuoteEO.VALID_FROM, 
       XxadsRentalQuoteEO.VALID_TO, 
       (select inv.bu_name from xxinv_business_units_t inv where inv.bu_id = XxadsRentalQuoteEO.REQ_BU_ID) AS TRANSREQUESTINGBU, 
       (select inv.bu_name from xxinv_business_units_t inv where inv.bu_id = XxadsRentalQuoteEO.BU_ID) AS TRANSSOURCEBU, 
       (select xlvt.meaning from xxcust_lookup_values_t xlvt where xlvt.lookup_type = 'XXADS_CUSTOMER_TYPES' and xlvt.lookup_code = XxadsRentalQuoteEO.CUSTOMER_TYPE) AS TRANSCUSTOMERTYPE, 
       (select xcdt.customer_name from xxfi_customer_details_t xcdt where xcdt.cust_account_id = XxadsRentalQuoteEO.CUSTOMER_ID) AS TRANSCUSTOMERNAME, 
       (SELECT xcsdt.bill_to_site||' - '||xcsdt.address from xxfi_customer_site_details_t xcsdt where xcsdt.site_use_id = XxadsRentalQuoteEO.SITE_USE_ID) AS TRANSCUSTOMERSITE, 
       (select xpj.project_number||' - '||xpj.project_name from xxpj_projects_t xpj where xpj.project_id = XxadsRentalQuoteEO.PROJECT_ID) AS TRANSPROJECT, 
       (select xlvt.meaning from xxcust_lookup_values_t xlvt where xlvt.lookup_type = 'XXADS_QUOTATION_STATUS' and xlvt.lookup_code = XxadsRentalQuoteEO.QUOTE_STATUS) AS TRANSQUOTATIONSTATUS, 
       (select fv.flex_value||' - '|| fv.description from fnd_flex_values_vl fv where fv.flex_value_id = XxadsRentalQuoteEO.DEPT_FLEX_VALUE_ID) AS TRANSDEPARTMENT, 
       (select fv.description from fnd_flex_values_vl fv where fv.flex_value_id = XxadsRentalQuoteEO.DIV_FLEX_VALUE_ID) AS TRANSCUSTDIVISION, 
       (select nvl((to_number(substr(quote_number,instr(quote_number,'-',-1)+1))),0) from dual) AS TRANSQUOTENUMBER, 
       XxadsRentalQuoteEO.LINE_TYPE"
  FromList="XXADS_RENTAL_QUOTE_T XxadsRentalQuoteEO"
  BindingStyle="OracleName"
  CustomQuery="false"
  RowClass="model.searchview.RentalQuotationSearchVORowImpl"
  ComponentClass="model.searchview.RentalQuotationSearchVOImpl"
  PageIterMode="Full"
  Where="XxadsRentalQuoteEO.QUOTE_TYPE = :p_quote_type        
and XxadsRentalQuoteEO.BU_ID = :p_bu_id        
and nvl(XxadsRentalQuoteEO.SRC_DIV_FLEX_VAL_ID,-2) = NVL(:p_src_div_flex_val_id,nvl(XxadsRentalQuoteEO.SRC_DIV_FLEX_VAL_ID,-2))        
and XxadsRentalQuoteEO.REQ_BU_ID = nvl(:p_req_bu_id, XxadsRentalQuoteEO.REQ_BU_ID)        
and XxadsRentalQuoteEO.QUOTE_NUMBER = nvl(:p_quote_number, XxadsRentalQuoteEO.QUOTE_NUMBER)        
and XxadsRentalQuoteEO.QUOTE_STATUS = nvl(:p_quote_status, XxadsRentalQuoteEO.QUOTE_STATUS)     
and XxadsRentalQuoteEO.CUSTOMER_TYPE = nvl(:p_customer_type, XxadsRentalQuoteEO.CUSTOMER_TYPE)        
and nvl(XxadsRentalQuoteEO.PROJECT_ID,-1) = nvl(:p_project_id, nvl(XxadsRentalQuoteEO.PROJECT_ID,-1))        
and nvl(XxadsRentalQuoteEO.DIV_FLEX_VALUE_ID,-1) = nvl(:p_div_flex_val_id, nvl(XxadsRentalQuoteEO.DIV_FLEX_VALUE_ID,-1))       
and nvl(XxadsRentalQuoteEO.DEPT_FLEX_VALUE_ID,-1) = nvl(:p_dept_flex_val_id, nvl(XxadsRentalQuoteEO.DEPT_FLEX_VALUE_ID,-1))       
and nvl(XxadsRentalQuoteEO.CUSTOMER_ID,-1) = nvl(:p_customer_id, nvl(XxadsRentalQuoteEO.CUSTOMER_ID,-1))       
and nvl(XxadsRentalQuoteEO.SITE_USE_ID,-1) = nvl(:p_site_use_id, nvl(XxadsRentalQuoteEO.SITE_USE_ID,-1))   
and exists (select xxads_process_pkg.validate_ads_access(XxadsRentalQuoteEO.CUSTOMER_TYPE,   
                                                                                        (case    
                                                                                            when    
                                                                                                XxadsRentalQuoteEO.CUSTOMER_TYPE = 'P'    
                                                                                            then    
                                                                                                XxadsRentalQuoteEO.PROJECT_ID   
                                                                                            when   
                                                                                                XxadsRentalQuoteEO.CUSTOMER_TYPE = 'C'   
                                                                                            then    
                                                                                                XxadsRentalQuoteEO.CUSTOMER_ID   
                                                                                            when    
                                                                                                XxadsRentalQuoteEO.CUSTOMER_TYPE = 'D'   
                                                                                            then   
                                                                                                XxadsRentalQuoteEO.DEPT_FLEX_VALUE_ID   
                                                                                            when   
                                                                                                XxadsRentalQuoteEO.CUSTOMER_TYPE = 'DV'   
                                                                                            then   
                                                                                                XxadsRentalQuoteEO.DIV_FLEX_VALUE_ID   
                                                                                            else   
                                                                                                null   
                                                                                        end),                                                                                        
                                                                                        XxadsRentalQuoteEO.BU_ID,    
                                                                                        XxadsRentalQuoteEO.SRC_DIV_FLEX_VAL_ID,   
                                                                                        XxadsRentalQuoteEO.REQ_BU_ID,    
                                                                                        :p_user_id)    
                        from dual)"
