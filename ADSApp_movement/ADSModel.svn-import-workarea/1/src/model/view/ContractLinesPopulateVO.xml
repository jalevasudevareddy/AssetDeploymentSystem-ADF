  <SQLQuery><![CDATA[select REF_LINE_ID,        
REF_LINE_NUM,        
TRANS_LINE_TYPE,        
LINE_TYPE,        
CATEGORY_NAME,        
CATEGORY_DESCRIPTION,        
CATEGORY_ID,        
QUANTITY,        
HIRING_BASIS,        
TRANS_FIXED_VARIABLE,        
HIRING_BASIS_RULE_ID,        
TRANS_OPERATOR_OPTION,        
OPERATOR_OPTION,        
TRANS_FUEL_OPTION,        
FUEL_OPTION,        
TRANS_MOBILIZATION_REQUIRED,        
MOBILIZATION_REQUIRED,        
TRANS_SHIFT,        
SHIFT,        
ON_HIRE_DATE,        
OFF_HIRE_DATE,        
STANDARD_RATE,        
OT1_RATE,        
OT2_RATE,        
TRANS_MIN_CHARGE_UNIT,        
MIN_CHARGE_UNIT,        
MIN_CHARGE_VALUE,        
REF_LINE_MASTER_ID,        
HEADER_ID,        
MASTER_HDR_ID ,       
LINE_SOURCE,       
TRANS_LINE_SOURCE ,      
tot_quantity,   
task_id,   
    task_name,   
    expenditure_type_id,   
    expenditure_type_desc,  
UNIT_RATE , 
NO_OF_DAYS, 
NO_OF_HOURS 
from xxads_req_quote_lines_v v     
where HEADER_ID = :p_reference_id       
and line_source = :p_contract_source      
and not exists (select 'Y'       
                from xxads_rental_contract_lines_t lns, xxads_rental_contract_t hdr       
                where lns.contract_id = hdr.contract_id       
                and hdr.contract_source = v.line_source       
                and lns.reference_id = v.header_id      
                and lns.reference_line_id = v.REF_LINE_ID      
                and (lns.contract_id = :p_contract_id      
                or (select sum(nvl(lns1.quantity,0)) from xxads_rental_contract_lines_t lns1, xxads_rental_contract_t hdr1       
                    where lns1.contract_id = hdr1.contract_id      
                    and lns1.reference_line_id = lns.reference_line_id) = v.tot_quantity      
                    )      
                )    
and v.quantity > 0]]></SQLQuery>
